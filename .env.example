# Copy this file to .env.local and fill in your values

# Required: Google Generative AI API Key
# Get yours at: https://makersuite.google.com/app/apikey
# Note: This is only read by the server/edge functions, not the browser
GOOGLE_GENERATIVE_AI_API_KEY=

# Vite public environment variables (accessible in browser)
# Must be prefixed with VITE_ to be exposed to the client
VITE_ENABLE_CHAT_PATTERNS=true
VITE_CHAT_RATE_LIMIT=10
VITE_API_URL=/api

# Optional: Vercel Analytics ID
VERCEL_ANALYTICS_ID=